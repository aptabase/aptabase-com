---
import "cal-sans";
import "@fontsource/inter";
import BaseLayout from "./BaseLayout.astro";

export interface Props {
  title: string;
  description: string;
  og?: string;
}

const { title, description, og } = Astro.props;
---

<BaseLayout title={title} description={description} og={og}>
  <main class="py-16">
    <slot />
  </main>
</BaseLayout>
